{
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "semi": 2,
        "comma-dangle": "off",
        "require-jsdoc": "off",
        "no-tabs": 0
    }
}